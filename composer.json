{
  "name": "sjerd/laravel-revenuecat",
  "version": "v0.0.1",
  "type": "library",
  "description": "Wrapper to perform basic tasks against the RevenueCat API",
  "keywords": [
    "Sjerd",
    "LaravelRevenuecat"
  ],
  "homepage": "https://github.com/Sjerd/LaravelRevenuecat",
  "license": "MIT",
  "authors": [
    {
      "name": "Sjoerd de Wit",
      "email": "info@sjerd.nl",
      "homepage": "https://sjerd.nl",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "~7.2",
    "guzzlehttp/guzzle": "^6.5"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Sjerd\\LaravelRevenueCat\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sjerd\\LaravelRevenueCat\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "master": "dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
